
# Set production fixes - Nose/Hoover
#
fix                 2 all npt temp ${TEMP} ${TEMP} ${TDAMP} iso ${PRES} ${PRES} ${PDAMP}
fix                 2 all npt temp 298 ${TEMP} ${TDAMP} iso ${PRES} ${PRES} ${PDAMP}
fix                 2 all npt temp 298 298 ${TDAMP} iso ${PRES} ${PRES} ${PDAMP}
fix                 2 all npt temp 298 298 1000.0 iso ${PRES} ${PRES} ${PDAMP}
fix                 2 all npt temp 298 298 1000.0 iso 1 ${PRES} ${PDAMP}
fix                 2 all npt temp 298 298 1000.0 iso 1 1 ${PDAMP}
fix                 2 all npt temp 298 298 1000.0 iso 1 1 2500.0
compute             2 all orientorder/atom nnn NULL cutoff 3.5

# Run 1ns simulation
dump		    1 all custom 10000 prod.${NAME}.dump id type x y z vx vy vz c_2[*]
dump		    1 all custom 10000 prod.water_box_298K_1atm_ML.dump id type x y z vx vy vz c_2[*]
run                 100000
