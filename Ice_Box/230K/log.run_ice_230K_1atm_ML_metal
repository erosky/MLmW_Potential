
# Set production fixes - Nose/Hoover
#
fix                 2 all nph iso ${PRES} ${PRES} ${PCOUPL}
fix                 2 all nph iso 1 ${PRES} ${PCOUPL}
fix                 2 all nph iso 1 1 ${PCOUPL}
fix                 2 all nph iso 1 1 10000.0
fix                 3 all temp/csvr ${TEMP} ${TEMP} ${TCOUPL} 837492
fix                 3 all temp/csvr 230 ${TEMP} ${TCOUPL} 837492
fix                 3 all temp/csvr 230 230 ${TCOUPL} 837492
fix                 3 all temp/csvr 230 230 5000.0 837492

# Define Order parameter
#
compute             2 all orientorder/atom nnn NULL cutoff 3.5
thermo              1000
thermo_style        custom step temp press vol etotal ke pe c_1[4]

# Run 0.5ns simulation
dump		    1 all custom 1000 prod.${NAME}.dump id type x y z vx vy vz c_2[*]
dump		    1 all custom 1000 prod.ice_230K_1atm_ML_metal.dump id type x y z vx vy vz c_2[*]
run                 100000
